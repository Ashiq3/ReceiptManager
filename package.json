{
    "name": "receipt-manager",
    "version": "1.0.0",
    "description": "AI-Powered Receipt Management System for Small Businesses",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "build": "npm run build:frontend && npm run build:backend",
        "build:frontend": "cd frontend && npm install && npm run build",
        "build:backend": "babel src --out-dir dist",
        "vercel-build": "npm run build:frontend",
        "test": "npm run test:unit && npm run test:integration",
        "test:unit": "jest --coverage",
        "test:integration": "jest --testPathPattern=integration",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "prepare": "husky install",
        "migrate": "knex migrate:latest",
        "rollback": "knex migrate:rollback",
        "seed": "knex seed:run"
    },
    "dependencies": {
        "@google/generative-ai": "^0.24.1",
        "@supabase/supabase-js": "^2.86.0",
        "axios": "^1.13.2",
        "bcrypt": "^5.1.1",
        "body-parser": "^1.20.2",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-rate-limit": "^8.2.1",
        "express-validator": "^7.3.1",
        "helmet": "^7.0.0",
        "jsonwebtoken": "^9.0.2",
        "knex": "^2.4.2",
        "multer": "^1.4.5-lts.1",
        "pg": "^8.10.0",
        "sharp": "^0.32.6",
        "uuid": "^9.0.0",
        "winston": "^3.18.3"
    },
    "devDependencies": {
        "@babel/cli": "^7.21.0",
        "@babel/core": "^7.21.4",
        "@babel/preset-env": "^7.21.4",
        "@babel/preset-typescript": "^7.21.4",
        "eslint": "^8.38.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.27.5",
        "husky": "^8.0.3",
        "jest": "^29.5.0",
        "nodemon": "^2.0.22",
        "supertest": "^6.3.3"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=8.0.0"
    },
    "keywords": [
        "receipt",
        "management",
        "ai",
        "ocr",
        "small-business",
        "expense-tracking"
    ],
    "author": "Receipt Manager Team",
    "license": "MIT",
    "private": true
}